package Exercicio2;

public class ObjAviao {

	public static void main(String[] args) {

		Aviao av;
		
		av = new Aviao();
		
		System.out.println("Escolha seu tipo de avião: ");
		
		System.out.println(av.A + "Avião de passeio");
		System.out.println(av.B + "Avião internacional");
		System.out.println(av.C + "Avião de guerra");

	}
}
